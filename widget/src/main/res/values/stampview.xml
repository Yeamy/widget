<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="StampView">
        <attr name="dotSize" format="dimension" />
        <attr name="distance" format="dimension" />
        <attr name="sides">
            <enum name="left" value="0x1" />
            <enum name="right" value="0x2" />
            <enum name="top" value="0x4" />
            <enum name="bottom" value="0x8" />
            <enum name="all" value="0xf" />
        </attr>
    </declare-styleable>

</resources>